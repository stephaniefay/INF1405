function createAttack (damage as integer, name as String)
	newAttack as attackStatus
	
	newAttack.damage = damage
	newAttack.name = name
	
	message(Str(newAttack.damage))
	message(newAttack.name)
	
endfunction newAttack

function getAttackDamage (attack as attackStatus)
endfunction attack.damage

function getAttackName (attack as attackStatus)
endfunction attack.name
